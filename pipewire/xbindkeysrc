"pactl set-sink-mute @DEFAULT_SINK@ toggle"
   XF86AudioMute

"pactl set-sink-volume @DEFAULT_SINK@ -1000"
   XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ 0; pactl set-sink-volume @DEFAULT_SINK@ +1000"
   XF86AudioRaiseVolume

"playerctl --ignore-player=Soundcore_Life_P2,Soundcore_Life_P2_L,Soundcore_Life_P2_RS play-pause"
   XF86AudioPlay

"playerctl --ignore-player=Soundcore_Life_P2,Soundcore_Life_P2_L,Soundcore_Life_P2_RS previous"
   XF86AudioPrev

"playerctl --ignore-player=Soundcore_Life_P2,Soundcore_Life_P2_L,Soundcore_Life_P2_RS next"
   XF86AudioNext

"playerctl --ignore-player=Soundcore_Life_P2,Soundcore_Life_P2_L,Soundcore_Life_P2_RS stop"
   XF86AudioStop
