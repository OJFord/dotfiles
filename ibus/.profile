dconf write /desktop/ibus/general/embed-preedit-text true
dconf write /desktop/ibus/general/engines-order "['xkb:us::eng', 'm17n:hi:itrans']"
dconf write /desktop/ibus/general/use-system-keyboard-layout true
dconf write /desktop/ibus/panel/show 0
dconf write /desktop/ibus/panel/show-icon-on-systray true
dconf write /desktop/ibus/panel/use-custom-font false

dconf write /org/freedesktop/ibus/engine/typing-booster/dictionary "'hi_IN,en_GB'"
dconf write /org/freedesktop/ibus/engine/typing-booster/inputmethod "'NoIME,hi-itrans'"
